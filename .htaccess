DirectoryIndex index.php

AddDefaultCharset UTF-8

RewriteEngine on

RewriteRule ^([A-z0-9-]+)/?$ index.php?mode=$1 [L]
RewriteRule ^$ index.php?do=main [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*).(.*)$ index.php [L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [L]
